<?xml version="1.0" encoding="utf-8"?>
<Defs>

  <ThingDef ParentName="ResourceBase">
    <defName>EXW_NeutroniumFragment</defName>
    <label>neutronium fragment</label>
    <description>Little fragments consisting of neutrons. Refine to get neutronium</description>
    <graphicData>
      <texPath>Things/Resource/NeutroniumFragment</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <soundInteract>Metal_Drop</soundInteract>
    <soundDrop>Standard_Drop</soundDrop>
    <healthAffectsPrice>false</healthAffectsPrice>
    <statBases>
      <MaxHitPoints>150</MaxHitPoints>
      <MarketValue>150</MarketValue>
      <Mass>0.8</Mass>
      <Flammability>0</Flammability>
    </statBases>
    <intricate>true</intricate>
    <thingCategories>
      <li>ResourcesRaw</li>
    </thingCategories>
  </ThingDef>
  
  
  <ThingDef ParentName="ResourceBase">
    <defName>EXW_Neutronium</defName>
    <label>neutronium</label>
    <description>The densest form of matter in the universe existing inside the neutron stars.</description>
    <possessionCount>2</possessionCount>
    <graphicData>
      <texPath>Things/Resource/Neutronium</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <soundInteract>Metal_Drop</soundInteract>
    <soundDrop>Standard_Drop</soundDrop>
    <stackLimit>75</stackLimit>
    <healthAffectsPrice>false</healthAffectsPrice>
    <statBases>
      <MaxHitPoints>150</MaxHitPoints>
      <MarketValue>1500</MarketValue>
      <Mass>0.05</Mass>
      <Flammability>0</Flammability>
    </statBases>
    <intricate>true</intricate>
    <thingCategories>
      <li>Manufactured</li>
    </thingCategories>
  </ThingDef>

</Defs>